-- Gui to Lua
-- Version: 3.2

local RELKINlibrary = {}

function RELKINlibrary:Create()
    local gui = Instance.new("ScreenGui")
    local Main = Instance.new("Frame")
    -- ... existing code for creating GUI elements ...
    
    -- Properties
    gui.Name = "RELKIN library"
    gui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
    gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    gui.ResetOnSpawn = false

    -- ... existing code for setting properties ...

    -- Scripts
    local function HRMIIOL_fake_script()
        -- Main.dragging script
        local script = Instance.new('LocalScript', Main)
        -- ... existing dragging script code ...
    end
    coroutine.wrap(HRMIIOL_fake_script)()

    local function LNKTZYF_fake_script()
        -- Close.Close script
        local script = Instance.new('LocalScript', Close)
        script.Parent.MouseButton1Click:Connect(function()
            script.Parent.Parent.Parent.Visible = false
        end)
    end
    coroutine.wrap(LNKTZYF_fake_script)()

    return gui
end

return RELKINlibrary
